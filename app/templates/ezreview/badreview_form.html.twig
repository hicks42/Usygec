{% extends 'ezreview_base.html.twig' %}

{% block title %}Retour de l'enquete de satisfaction.
{% endblock %}
{% block javascripts %}
	{{ parent() }}
{% endblock javascripts %}

{% block content %}
	<div class="container">
		<div class="row mt-3">
			<div class="col-lg-12 co-12">
				<div class="form-container">
					<h3>Faites part de votre m√©contentement :</h3>
					{{ form_start(form) }}
					<div class="row mt-3">
						<div class="col-md-12">
							{{ form_row(form.email) }}
						</div>
					</div>
					<div class="row mt-3">
						<div class="col-md-12">
							{{ form_row(form.lieu_rdv) }}
						</div>
					</div>
					<div class="row mt-3">
						<div class="col-md-6 ">
							{{ form_row(form.date_rdv) }}
						</div>
						<div class="col-md-6">
							{{ form_row(form.note) }}
						</div>
					</div>
					<div class="row mt-3">
						<div class="col-md-12">
							{{ form_row(form.message) }}
						</div>
					</div>
					{{ form_end(form) }}
				</div>
				{{ include('main/partials/_connexion.html.twig') }}
			</div>
		</div>
	</div>
{% endblock %}
